{
  "info": {
    "name": "Delete Comment",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Delete Comment",
      "request": {
        "method": "DELETE",
        "header": [
          {
            "key": "Authorization",
            "value": "Token {{token}}"
          }
        ],
        "url": {
          "raw": "{{baseUrl}}/articles/:slug/comments/:id",
          "host": ["{{baseUrl}}"],
          "path": ["articles", ":slug", "comments", ":id"]
        }
      }
    }
  ],
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:8080"
    },
    {
      "key": "token",
      "value": ""
    }
  ]
}
